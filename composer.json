{
    "name": "jaulz/hoard",
    "description": "Hoard helps you to automatically keep cached values to reduce reads in your database.",
    "keywords": ["laravel", "eloquent", "cache", "count", "max", "min", "sum", "hoard", "remember"],
    "authors": [
        {
            "name": "Julian Hundeloh",
            "email": "julian@hundeloh-consulting.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0 || ^10.0",
        "spatie/enum": "^3.13"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.0",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jaulz\\Hoard\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jaulz\\Hoard\\HoardServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
